{
  "bindings": {
    "/actions/main": {
      "haptics": [
        {
          "output": "/actions/main/out/left_slice_haptics",
          "path": "/user/hand/left/output/haptic"
        },
        {
          "output": "/actions/main/out/right_slice_haptics",
          "path": "/user/hand/right/output/haptic"
        }
      ],
      "poses": [
        {
          "output": "/actions/main/in/left_hand_pose",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/main/in/right_hand_pose",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "inputs": {
            "pull": {
              "output": "/actions/main/in/left_trigger"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/left/input/trigger"
        },
        {
          "inputs": {
            "pull": {
              "output": "/actions/main/in/right_trigger"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/right/input/trigger"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/left_menu_button"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/b"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/main/in/right_menu_button"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/b"
        },
        {
          "inputs": {
            "position": {
              "output": "/actions/main/in/left_thumbstick"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/left/input/thumbstick"
        },
        {
          "inputs": {
            "position": {
              "output": "/actions/main/in/right_thumbstick"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/right/input/thumbstick"
        }
      ]
    },
    "/actions/unity": {
      "haptics": [
        {
          "output": "/actions/unity/out/left_haptics",
          "path": "/user/hand/left/output/haptic"
        },
        {
          "output": "/actions/unity/out/right_haptics",
          "path": "/user/hand/right/output/haptic"
        }
      ],
      "poses": [
        {
          "output": "/actions/unity/in/left_pose",
          "path": "/user/hand/left/pose/raw"
        },
        {
          "output": "/actions/unity/in/right_pose",
          "path": "/user/hand/right/pose/raw"
        }
      ],
      "sources": [
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/left_trigger_button"
            },
            "pull": {
              "output": "/actions/unity/in/left_trigger"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/left/input/trigger"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/right_trigger_button"
            },
            "pull": {
              "output": "/actions/unity/in/right_trigger"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/right/input/trigger"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/left_primary_2d_axis_click"
            },
            "position": {
              "output": "/actions/unity/in/left_primary_2d_axis"
            },
            "touch": {
              "output": "/actions/unity/in/left_primary_2d_axis_touch"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/left/input/thumbstick"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/right_primary_2d_axis_click"
            },
            "position": {
              "output": "/actions/unity/in/right_primary_2d_axis"
            },
            "touch": {
              "output": "/actions/unity/in/right_primary_2d_axis_touch"
            }
          },
          "mode": "joystick",
          "path": "/user/hand/right/input/thumbstick"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/left_secondary_button"
            },
            "touch": {
              "output": "/actions/unity/in/left_secondary_touch"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/b"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/right_secondary_button"
            },
            "touch": {
              "output": "/actions/unity/in/right_secondary_touch"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/b"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/left_primary_button"
            },
            "touch": {
              "output": "/actions/unity/in/left_primary_touch"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/a"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/right_primary_button"
            },
            "touch": {
              "output": "/actions/unity/in/right_primary_touch"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/a"
        },
        {
          "inputs": {
            "pull": {
              "output": "/actions/unity/in/left_grip"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/left/input/grip"
        },
        {
          "inputs": {
            "pull": {
              "output": "/actions/unity/in/right_grip"
            }
          },
          "mode": "trigger",
          "path": "/user/hand/right/input/grip"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/left_grip_button"
            }
          },
          "mode": "button",
          "path": "/user/hand/left/input/grip"
        },
        {
          "inputs": {
            "click": {
              "output": "/actions/unity/in/right_grip_button"
            }
          },
          "mode": "button",
          "path": "/user/hand/right/input/grip"
        }
      ]
    }
  },
  "controller_type": "knuckles"
}